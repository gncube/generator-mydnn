{
  "name": "generator-mydnn",
  "version": "1.0.0",
  "description": "DNN scaffolding generator",
  "main": "generators/index.js",
  "files": [
    "generators"
  ],
  "scripts": {
    "test": "jest",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "linebreak-style": 0,
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gncube/generator-mydnn.git"
  },
  "keywords": [
    "yeoman-generator",
    "dnn",
    "dnn-cli",
    "gsn"
  ],
  "author": "Gerald Ncube of GSN",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gncube/generator-mydnn/issues"
  },
  "homepage": "https://github.com/gncube/generator-mydnn#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "dotnet-solution": "^0.2.0",
    "pascal-case": "^3.1.1",
    "update-notifier": "^5.0.1",
    "uuid-v4": "^0.1.0",
    "which": "^2.0.2",
    "yeoman-generator": "^4.12.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-xo": "^0.33.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "nsp": "^3.2.1",
    "prettier": "^2.2.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^3.0.0",
    "yo": "^3.1.1"
  }
}
