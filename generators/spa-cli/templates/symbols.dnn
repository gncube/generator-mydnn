<?xml version="1.0" encoding="utf-8"?>
<dotnetnuke type="Package" version="6.0">
  <packages>
    <package name="<%= fullNamespace %>_Symbols" type="Library" version="01.00.00">
      <friendlyName><%= namespace %>: <%= extensionName %> Module Symbols</friendlyName>
      <description><![CDATA[<%= description %>]]></description>
      <owner>
        <name><%= yourName %></name>
        <organization><%= company %></organization>
        <url><%= companyUrl %></url>
        <email><%= emailAddy %></email>
      </owner>
      <license src="License.txt" />
      <releaseNotes src="ReleaseNotes.txt" />
      <azureCompatible>True</azureCompatible>
      <dependencies>
        <dependency type="managedPackage" version="1.0.0"><%= fullNamespace %></dependency>
      </dependencies>

      <components>
        <component type="ResourceFile">
          <resourceFiles>
            <basePath></basePath>
            <resourceFile>
              <name>Resources.zip</name>
            </resourceFile>
          </resourceFiles>
        </component>
      </components>
    </package>
  </packages>
</dotnetnuke>
